<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/properties_screen"
    >

    <fragment
        android:id="@+id/properties_screen"
        android:name="dev.filipebezerra.app.propertiesfrommars.properties.PropertiesScreen"
        android:label="@string/properties_fragment_label"
        tools:layout="@layout/properties_screen"
        >

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="dev.filipebezerra.app.propertiesfrommars.propertydetail.PropertyDetailScreen"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/property_detail_screen"
        >

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/properties_screen"
            />
    </fragment>
</navigation>