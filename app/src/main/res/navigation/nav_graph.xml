<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/properties_screen"
    >
    <fragment
        android:id="@+id/properties_screen"
        android:name="dev.filipebezerra.app.propertiesfrommars.properties.PropertiesScreen"
        android:label="@string/properties_screen_label"
        tools:layout="@layout/properties_screen"
        >
        <action
            android:id="@+id/action_properties_screen_to_property_detail_screen"
            app:destination="@id/property_detail_screen"
            />
    </fragment>
    <dialog
        android:id="@+id/property_detail_screen"
        android:name="dev.filipebezerra.app.propertiesfrommars.propertydetail.PropertyDetailScreen"
        android:label="@string/property_detail_screen_label"
        tools:layout="@layout/property_detail_screen"
        >
        <argument
            android:name="property"
            app:argType="dev.filipebezerra.app.propertiesfrommars.domain.MarsProperty"
            />
    </dialog>
</navigation>